-
-#define UART_BASE		0xbe100c00
-#define ASC_TBUF		(UART_BASE | 0x20)
-#define ASC_FSTAT		(UART_BASE | 0x48)
-
-#define TXMASK          0x3F00
-#define TXOFFSET        8
-
-void board_putc(char c)
-{
-	while ((READREG(ASC_FSTAT) & TXMASK) >> TXOFFSET);
-
-	WRITEREG(ASC_TBUF, c);
-}
