--- openwrt.bb/openwrt/target/linux/mpc85xx/base-files/etc/uci-defaults/02_network	2015-08-15 05:48:44.111797702 +0200
+++ witi-openwrt/target/linux/mpc85xx/base-files/etc/uci-defaults/02_network	1970-01-01 01:00:00.000000000 +0100
@@ -1,33 +0,0 @@
-#!/bin/sh
-# Copyright (C) 2014 OpenWrt.org
-
-[ -e /etc/config/network ] && exit 0
-
-touch /etc/config/network
-
-. /lib/functions/uci-defaults.sh
-. /lib/mpc85xx.sh
-. /lib/functions.sh
-. /lib/functions/system.sh
-
-ucidef_set_interface_loopback
-
-board=$(mpc85xx_board_name)
-
-case "$board" in
-tl-wdr4900-v1)
-	ucidef_set_interfaces_lan_wan "eth0.1" "eth0.2"
-	ucidef_add_switch "switch0" "1" "1"
-	ucidef_add_switch_vlan "switch0" "1" "0t 2 3 4 5"
-	ucidef_add_switch_vlan "switch0" "2" "0t 1"
-	ucidef_set_interface_macaddr lan "$(mtd_get_mac_binary config 338)"
-	ucidef_set_interface_macaddr wan "$(mtd_get_mac_binary config 344)"
-	;;
-*)
-	ucidef_set_interfaces_lan_wan "eth0" "eth1"
-	;;
-esac
-
-uci commit network
-
-exit 0
