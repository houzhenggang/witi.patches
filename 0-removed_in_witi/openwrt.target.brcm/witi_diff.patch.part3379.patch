-START=41
-
-boot() {
-	[ -d /sys/class/ieee80211 ] || exit
-
-	commit=0
-
-	fixup_wmac() {
-		local cfg="$1"
-		local cfmac
-
-		config_get cfmac "$cfg" macaddr
-
