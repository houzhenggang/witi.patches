--- openwrt.bb/openwrt/target/linux/brcm63xx/base-files/etc/hotplug.d/firmware/10-rt2x00-eeprom	2015-08-15 05:48:43.955131038 +0200
+++ witi-openwrt/target/linux/brcm63xx/base-files/etc/hotplug.d/firmware/10-rt2x00-eeprom	1970-01-01 01:00:00.000000000 +0100
@@ -1,42 +0,0 @@
-#!/bin/sh
-# Based on gabors ralink wisoc implementation.
-
-rt2x00_eeprom_die() {
-	echo "rt2x00 eeprom: " "$*"
-	exit 1
-}
-
-rt2x00_eeprom_extract() {
-	local part=$1
-	local offset=$2
-	local count=$3
-	local mtd
-
-	. /lib/functions.sh
-
-	mtd=$(find_mtd_part $part)
-	[ -n "$mtd" ] || \
-		rt2x00_eeprom_die "no mtd device found for partition $part"
-
-	dd if=$mtd of=/lib/firmware/$FIRMWARE bs=1 skip=$offset count=$count || \
-		rt2x00_eeprom_die "failed to extract from $mtd"
-}
-
-[ -e /lib/firmware/$FIRMWARE ] && exit 0
-
-. /lib/brcm63xx.sh
-
-board=$board_name
-
-case "$FIRMWARE" in
-"rt2x00.eeprom" )
-	case $board in
-	HW556_A)
-		rt2x00_eeprom_extract "cal_data" 130560 512
-		;;
-	*)
-		rt2x00_eeprom_die "board $board is not supported yet"
-		;;
-	esac
-	;;
-esac
